[daemon]
; Set the location of the private key to be used
; in this 
privateKeyLocation=example-key/key
; Set the port this server should run on.
port=2222
; Git repository path
reposiotryPath=/git

[remote-auth-server]
; Likely the path to your Drupal site.
url=http://syssettings.local
; TODO: update this once we actually have this service living in the right module
path=vcs-git-auth

[beanstalkd-git-repo-manager-daemon]
repoisotryPath=/git
; The machine 
url=127.0.0.1
port=11300
